{
  "rangeStrategy": "pin",
  "masterIssue": true,
  "prConcurrentLimit": 3,
  "separateMultipleMajor": true,
  "packageRules": [
    {
      "groupName": "Node",
      "matchPackageNames": ["cimg/node", "node"]
    },
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchDepTypes": ["devDependencies"],
      "automerge": true
    }
  ],
  "encrypted": {
    "npmToken": ""
  },
  "npmrc": "//registry.npmjs.org/:_authToken=${NPM_TOKEN}"
}
